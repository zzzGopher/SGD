{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"inlineSourceMap": true,
		"sourceMap": true,
		"outDir": "./dist",
		"strict": true,
		"ignoreDeprecations": "5.0"

	}
}
